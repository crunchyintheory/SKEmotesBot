{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "react/jsx-sort-prop-types": "off",
        "no-invalid-regexp": "error",
        "no-extra-parens": "error",
        "no-extra-semi": "error",
        "no-regex-spaces": "warn",
        "no-unreachable": "error",
        "no-unexpected-multiline": "error"
    }
}